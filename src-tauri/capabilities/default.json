{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": [
            "folder_path",
            "app_data_path",
            "treat_toplevel_folders_as_one"
          ],
          "name": "Organize_Folder",
          "sidecar": true
        }
      ]
    },
    "shell:allow-open"
  ]
}
